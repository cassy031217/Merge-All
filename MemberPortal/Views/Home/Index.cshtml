<!-- Start Main Content Wrapper -->
<div class="main-content-wrapper">
    <div id="about" class="content-section-area ptb-80">
        <div class="container">
            <div class="row">
                <div class="col-md-8 col-sm-8 col-md-offset-2 col-sm-offset-2">
