
@{
    ViewBag.Title = "General Assembly 2019 ";
    Layout = "~/Views/Shared/_layout.cshtml";
}

<div class="banner-area banner-bg-3 ptb-80 bg-opacity-black-50 ">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <div class="banner">
                    <h2>News & Events</h2>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="content-section-area ptb-80 white-bg bg-wabe1">
    <div class="container">
        <div class="row">
            <div class="col-lg-12">

                <hr />
                <div class="container">
                    <img src="~/Img/news/assembly.png" class="featurette-image img-fluid mx-auto" alt="Generic placeholder image" width=80%" height="500" />
                </div>
                <br />
                <p class="text-justify">
                    <h2><b>NEC General Assembly 2019</b></h2>
                </p>
                <p>Congratulations to all the winners last General Assembly held at Daet, Camarines Norte. Till next year.</p>
                <div class="col-md-4">
                    <div class=table-responsive>
                        <table class="table">
                            <colgroup>
                                <col class=col-xs-3>
                                <col class=col-xs-7>
                            </colgroup>
                            <thead>
                                <tr>
                                    <th></th>
                                    <th></th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr>
                                    <th scope=row>What</th>
                                    <td align="left">NEC General Assembly 2019 </td>
                                </tr>
                                <tr>
                                    <th scope=row>Where</th>
                                    <td align="left">Daet, Camarines Norte, Bicol </td>
                                </tr>
                                <tr>
                                    <th scope=row>When</th>
                                    <td align="left">March 30 - 31, 2019</td>
                                </tr>
                                <tr>
                                    <th></th>
                                    <td></td>
                                </tr>
                            </tbody>
                        </table>
                        <small class="d-block text-left mt-3">
                            <a href="@Url.Action("Index", "News")">More News</a>
                        </small>
                    </div>
                </div>
               
            </div>
        </div>
    </div>
</div>
